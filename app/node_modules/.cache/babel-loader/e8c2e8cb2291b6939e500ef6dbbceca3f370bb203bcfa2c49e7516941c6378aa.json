{"ast":null,"code":"export default {\n  name: \"Home\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 2,\n      collapseBtnClass: 'el-icon-s-fold',\n      isCollapse: false,\n      sideWidth: 200,\n      logoTextShow: true,\n      headerBg: 'headerBg'\n    };\n  },\n  created() {\n    this.getData();\n  },\n  methods: {\n    getData() {\n      this.request.get(\"http://localhost:9090/user/page?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize).then(res => {\n        console.log(res);\n        this.tableData = res.pageData;\n        this.total = res.total;\n      });\n    },\n    collapse() {\n      // 点击收缩按钮触发\n      this.isCollapse = !this.isCollapse;\n      if (this.isCollapse) {\n        // 收缩\n        this.sideWidth = 64;\n        this.collapseBtnClass = 'el-icon-s-unfold';\n        this.logoTextShow = false;\n      } else {\n        // 展开\n        this.sideWidth = 200;\n        this.collapseBtnClass = 'el-icon-s-fold';\n        this.logoTextShow = true;\n      }\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.getData();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.getData();\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","total","pageNum","pageSize","collapseBtnClass","isCollapse","sideWidth","logoTextShow","headerBg","created","getData","methods","request","get","then","res","console","log","pageData","collapse","handleSizeChange","handleCurrentChange"],"sources":["src/views/Home.vue"],"sourcesContent":["<template>\n  <el-container style=\"border: 1px solid #eee; min-height:100vh\">\n    <el-aside :width=\"sideWidth + 'px'\" style=\"background-color: rgb(238, 241, 246); overflow-x: hidden; box-shadow: 2px 0 6px rgb(0 21 41 / 35%);\">\n      <el-menu style=\"min-height:100%; overflow-x: hidden\"\n              background-color=\"rgb(48, 65, 86)\"\n              text-color=\"#fff\"\n             